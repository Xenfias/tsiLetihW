							end
						end)
					else
						EGod:Disconnect()
						if game.Players.LocalPlayer.PlayerGui:FindFirstChild("Popup") then
							game.Players.LocalPlayer.PlayerGui.Popup:Destroy()
						end
					end
			  end)


