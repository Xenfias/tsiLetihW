local GodModeBool = true
 
run.RenderStepped:connect(function()
    if GodModeBool then
        workspace.Touchy.Part.CFrame = plr.Character.HumanoidRootPart.CFrame + Vector3.new(0,0,1)
        if plr.PlayerGui:FindFirstChild("Popup") then
            plr.PlayerGui.Popup.Enabled = false
        end
        if not GodModeBool then
            return
        end
    end
end)
end
 
