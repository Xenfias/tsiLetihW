local Settings = enticzhub:addPage("Settings", 5012544693)
local Settings1 = Settings:addSection("Settings")
for theme, color in pairs(themes) do -- all in one theme changer, i know, im cool
Settings1:addColorPicker(theme, color, function(color3)
enticzhub:setTheme(theme, color3)
end)
end

Settings1:addKeybind("Toggle Keybind", Enum.KeyCode.RightShift, function()
enticzhub:toggle()
end, function()
end)
enticzhub:SelectPage(enticzhub.pages[1], true)
end

