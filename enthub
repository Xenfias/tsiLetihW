Other2:addButton("Grab Movespam", function()
    startGui:SetCore('SendNotification',{
Title = "Enticz cmds",
Text = "Press O to use Grab and P to activate and deactivate movespam...",
})
local plr = game:GetService("Players").LocalPlayer
local mouse = plr:GetMouse()

mouse.KeyDown:connect(function(key)
    if key == "o" then
game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack["Dragon Crush"])
game.Players.LocalPlayer.Character["Dragon Crush"]:Activate()
game.Workspace.Live[plr.Name]["Dragon Crush"].Activator["Flip"]:Destroy()
end
end)
MoveSpamBind = "p"
Moves =  {"Demon Flash", 
        "Neo Wolf Fang Fist", 
        "Flash Skewer", 
        "Sweep Kick", 
        "Burning Blast", 
        "TS Molotov", 
        "Blaster Meteor", 
        "Anger Rush", 
        "Meteor Crash", 
        "Big Bang Kamehameha"}

local baka = game:GetService("Players").LocalPlayer
local mouse = baka:GetMouse()
_G.Toggle = false

mouse.KeyDown:Connect(function(Key)
    if Key == MoveSpamBind then
        if not _G.Toggle then
            _G.Toggle = true
            while _G.Toggle == true do
                wait()
                for i, v in pairs(baka.Backpack:GetChildren()) do
                       if table.find( Moves, v.Name) then
                               v.Parent = baka.Character
                               v:Activate()
                               wait(0.1)
                               v:Deactivate()
                               v.Parent = baka.Backpack
                          end 
                    end
               end
        else
            _G.Toggle = false
        end
    end
end) 
end)

